#!/usr/bin/python3
# add_tuple
# a function that adds 2 tuples


def add_tuple(tuple_a=(), tuple_b=()):
    if len(tuple_a) < 2:
        if len(tuple_a) == 0:
            turple_a = 0, 0
        else:
            turple_a = turple_a[0], 0
    if len(tuple_b) < 2:
        if len(tuple_b) == 0:
            turple_b = 0, 0
        else
            turple_a = turple_a[0], 0
        
    
